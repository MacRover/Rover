<?xml version='1.0'?>

<robot name="rover" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find simulation)/robot/rover_drive.gazebo" />
  <!-- <xacro:include filename="$(find simulation)/robot/arm.xacro" /> -->
  <!-- <origin rpy="0 0 1.5707" xyz="0 0 0"/> -->
  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <collision name='collision'>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.9 0.58 0.2"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='chassis_visual'>
      <!-- <pose>-0.25 0.18 -0.04 0 0 0</pose> -->
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://simulation/robot/chassis_only.dae"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="20.0"/>
      <origin xyz="0.15 0 0" rpy=" 0 0 0"/>
      <!-- <inertia
          ixx="1.0" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="1.0"
      /> -->
      <inertia
          ixx="0.627" ixy="0" ixz="0"
          iyy="1.72" iyz="0"
          izz="1.91"/>
    </inertial>
  </link>
  
  <link name='upper_right_suspension'>
    <pose>0 0 0 0 0 0 </pose>

    <collision name='collision'>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://simulation/robot/upper_sus.dae"/>
      </geometry>>
    </collision>

    <visual name='visual'>
      <!-- <pose>-0.25 0.18 -0.04 0 0 0</pose> -->
      <!-- <origin rpy="0 0 0" xyz="0 0 -0.04"/> -->
      <geometry>
        <mesh filename="package://simulation/robot/upper_sus.dae"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="1.0"/>
      <origin xyz="0.05 0 0" rpy=" 0 0 0"/>
      <inertia
        ixx="1.0" ixy="0.0" ixz="0.0"
        iyy="1.0" iyz="0.0"
        izz="1.0"/>
    </inertial>
  </link>

  <joint name="upper_right_sus_joint" type="revolute">
    <origin xyz="0.1212 -0.3702 -0.05" rpy="0 0 3.14"/>
    <limit upper="0.57" lower="-0.57" velocity="0.5" effort="0.5"/>
    <axis xyz="0 1 0" />
    <parent link="chassis"/>
    <child link="upper_right_suspension"/>
  </joint>

  <link name='upper_left_suspension'>
