<?xml version='1.0'?>

<robot name="rover_with_arm" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:include filename="$(find simulation)/robot/rover.gazebo" />
  <xacro:include filename="$(find simulation)/robot/arm.xacro" />
  <!-- <xacro:include filename="$(find simulation)/robot/sensors/_d435.urdf.xacro" /> -->
  <xacro:include filename="$(find simulation)/robot/sensors/d435.xacro" />

  <origin rpy="0 0 1.5707" xyz="0 0 0"/>
  <link name='chassis'>
    <pose>0 0 0.1 0 0 0</pose>

    <collision name='collision'>
      <origin rpy="0 0 1.5707" xyz="0 0 -0.1"/>
      <geometry>
        <box size="0.7 1.0 0.3"/>
      </geometry>
      <surface>
        <friction>
          <ode>
            <mu>0</mu>
            <mu2>0</mu2>
            <slip1>1.0</slip1>
            <slip2>1.0</slip2>
          </ode>
        </friction>
      </surface>
    </collision>

    <visual name='chassis_visual'>
      <!-- <pose>-0.25 0.18 -0.04 0 0 0</pose> -->
      <origin rpy="0 0 0" xyz="0 0 -0.04"/>
      <geometry>
        <mesh filename="package://simulation/robot/chassis_with_wheels.dae"/>
      </geometry>
    </visual>
    <inertial>
      <mass value="20.0"/>
      <origin xyz="0 0 -0.1" rpy=" 0 0 1.5707"/>
      <inertia
          ixx="1.0" ixy="0" ixz="0"
          iyy="1.0" iyz="0"
          izz="0.5"
      />
    </inertial>
  </link>
  
  <!-- Arm joint -->
  <joint type="fixed" name="arm_joint">
    <origin xyz="0.3372 0 0.0637" rpy="0 0 0"/>
    <child link="arm_base_link"/>
    <parent link="chassis"/>
  </joint>

  <!-- Stereo camera -->
  <!-- <xacro:sensor_d435 parent="chassis" use_nominal_extrinsics="true" add_plug="false">
